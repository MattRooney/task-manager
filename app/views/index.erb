<div class="container">
  <div class="row">
    <div class=" col-md-6 col-md-offset-3 well well-sm text-center">
      <h1>All Tasks</h1>
      <% @tasks.each do |task| %>
      <h3><%= task.id %>: <a href="/tasks/<%= task.id %>"><%= task.title.capitalize %></a></h3>
      <% end %>
    </div>
  </div>
</div>
